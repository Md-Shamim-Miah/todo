<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="local/todo/db" VERSION="2024050712" COMMENT="XMLDB file for Moodle local/todo">
    <TABLES>
        <TABLE NAME="local_student_details" COMMENT="Table for storing student details">
            <FIELDS>
                <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="true" COMMENT="Primary key"/>
                <FIELD NAME="name" TYPE="char" LENGTH="100" NOTNULL="true" DEFAULT="" COMMENT="Student name"/>
                <FIELD NAME="roll" TYPE="char" LENGTH="20" NOTNULL="true" DEFAULT="" COMMENT="Roll number"/>
                <FIELD NAME="email" TYPE="char" LENGTH="100" NOTNULL="true" DEFAULT="" COMMENT="Student email"/>
            </FIELDS>
            <KEYS>
                <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
            </KEYS>
        </TABLE>
    </TABLES>
</XMLDB>
